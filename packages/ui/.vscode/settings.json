{
	"tailwindCSS.experimental.classRegex": [
		"className\\: '([^']*)'",
		"tw`([^`]*)`",
		["cva\\(([^)]*)\\)", "[\"'`]([^\"'`]*).*?[\"'`]"],
		["cx\\(([^)]*)\\)", "(?:'|\"|`)([^']*)(?:'|\"|`)"],
		["cn\\(([^)]*)\\)", "(?:'|\"|`)([^']*)(?:'|\"|`)"]
	],
	"files.associations": {
		"*.css": "tailwindcss"
	},
	"search.exclude": {
		"**/node_modules": true,
		"pnpm-lock.json": true,
		"pnpm-lock.yaml": true
	},
	"i18n-ally.localesPaths": ["public/locales"],
	"i18n-ally.keystyle": "flat",
	// "typescript.preferences.autoImportFileExcludePatterns": ["**/node_modules/**/*", "**/umbreld/**/*"]
	"typescript.preferences.importModuleSpecifier": "non-relative",
	"i18n-ally.usage.scanningIgnore": [
		"src/components/ui/debug-only.tsx",
		"src/routes/settings/_components/app-store-preferences-content.tsx"
	],
	"i18n-ally.extract.ignoredByFiles": {
		"src/components/cmdk.tsx": ["Enter"],
		"src/routes/live-usage.tsx": ["Live usage stubbed out for bitcoin, lightning, and nostr"],
		"src/trpc/trpc.ts": ["Bearer ${jwt}"]
	},
	"i18n-ally.extract.parsers.babel": {
		"function": "cn"
	},
	"i18n-ally.extract.ignored": ["h:mm aaa Â· MMM d"],
	"typescript.tsdk": "node_modules/typescript/lib",
	"i18n-ally.extract.autoDetect": true
}
